{
  "name": "phreshphoods",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start:backend": "node backend/server.js",
    "dev:full": "npm-run-all --parallel dev start:backend",
    "build:frontend": "tsc -b && vite build",
    "build:backend": "echo 'No backend build step defined (assuming plain JS or handled otherwise)'",
    "build": "npm run build:frontend && npm run build:backend",
    "lint": "eslint .",
    "preview": "vite preview",
    "process-data": "node backend/processSheetData.js",
    "process-data:farms": "node backend/processSheetData.js --type=farms",
    "process-data:cheese": "node backend/processSheetData.js --type=cheese",
    "process-data:fish": "node backend/processSheetData.js --type=fish",
    "process-data:butchers": "node backend/processSheetData.js --type=butchers",
    "process-data:antiques": "node backend/processSheetData.js --type=antiques",
    "process-data:breweries": "node backend/processSheetData.js --type=breweries",
    "process-data:wineries": "node backend/processSheetData.js --type=wineries",
    "process-data:sugar-shacks": "node backend/processSheetData.js --type=sugar-shacks",
    "optimize-images": "node scripts/generate-webp.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "helmet": "^8.1.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/dompurify": "^3.0.5",
    "@types/google.maps": "^3.58.1",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "happy-dom": "^20.0.10",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.2.0",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "sharp": "^0.34.5",
    "sharp-cli": "^5.2.0",
    "tailwindcss": "3.3.4",
    "typescript": "^5.4.5",
    "vite": "^6.3.5",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.13",
    "workbox-window": "^7.4.0"
  }
}
